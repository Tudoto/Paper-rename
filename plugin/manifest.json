{
    "manifest_version": 3,
    "name": "PaperRename",
    "description": "Rename your paper",
    "version": "1.0",
    "permissions": [
        "downloads",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}